{
  "metadata": {
    "blueprintId": "typescript-react",
    "blueprintName": "TypeScript React Blueprint",
    "description": "Web application development with TypeScript and React",
    "version": "1.0.0",
    "author": "Cursor Agent Factory",
    "tags": ["typescript", "react", "frontend", "web"]
  },
  "stack": {
    "primaryLanguage": "typescript",
    "frameworks": [
      {"name": "React", "version": "19", "purpose": "UI framework"},
      {"name": "Vite", "version": "5+", "purpose": "Build tool"},
      {"name": "React Query", "version": "5+", "purpose": "Data fetching"},
      {"name": "Zustand", "version": "4+", "purpose": "State management"}
    ],
    "tools": [
      {"name": "TypeScript", "version": "5.6+", "purpose": "Type system"},
      {"name": "vitest", "purpose": "Testing"},
      {"name": "eslint", "purpose": "Linting"},
      {"name": "prettier", "purpose": "Formatting"}
    ],
    "styleGuides": [
      {"name": "airbnb", "description": "Airbnb JavaScript Style Guide"},
      {"name": "react", "description": "React Best Practices"}
    ]
  },
  "agents": [
    {"patternId": "code-reviewer", "required": true},
    {"patternId": "test-generator", "required": true, "customizations": {"skills": ["tdd", "bdd"]}},
    {"patternId": "explorer", "required": false}
  ],
  "skills": [
    {"patternId": "bugfix-workflow", "required": true},
    {"patternId": "feature-workflow", "required": true},
    {"patternId": "tdd", "required": true, "default": true},
    {"patternId": "bdd", "required": false, "default": false, "description": "Optional BDD for stakeholder collaboration"},
    {"patternId": "test-translation", "required": false, "default": false, "description": "Translation and traceability between BDD/TDD"},
    {"patternId": "code-templates", "required": true}
  ],
  "testingApproach": {
    "modes": ["tdd-only", "bdd-only", "layered", "bdd-drives-tdd", "tdd-documents-bdd", "synchronized"],
    "default": "tdd-only",
    "frameworks": {
      "tdd": "vitest",
      "bdd": "cucumber-js"
    },
    "translation": {
      "bddToTdd": {"enabled": false},
      "tddToBdd": {"enabled": false}
    },
    "traceability": {"enabled": false}
  },
  "knowledge": [
    {"filename": "naming-conventions.json", "description": "TypeScript/React naming conventions"},
    {"filename": "component-patterns.json", "description": "React 19 component patterns and best practices"},
    {"filename": "test-patterns.json", "description": "Vitest/RTL patterns"},
    {"filename": "bdd-patterns.json", "description": "Behavior-driven development patterns", "optional": true}
  ],
  "templates": {
    "codeTemplates": [
      {"category": "component", "directory": "templates/typescript/component/"},
      {"category": "hook", "directory": "templates/typescript/hook/"},
      {"category": "service", "directory": "templates/typescript/service/"},
      {"category": "test", "directory": "templates/typescript/test/"}
    ],
    "documentTemplates": [
      {"name": "implementation_plan.md", "directory": "templates/docs/"},
      {"name": "component_spec.md", "directory": "templates/docs/"}
    ]
  },
  "mcpServers": [
    {"name": "atlassian", "purpose": "Jira/Confluence", "required": false},
    {"name": "deepwiki", "purpose": "GitHub analysis", "required": false}
  ],
  "projectStructure": {
    "directories": [
      {"path": ".cursor/agents/", "purpose": "AI agent definitions"},
      {"path": ".cursor/skills/", "purpose": "Reusable skills"},
      {"path": "knowledge/", "purpose": "Reference data"},
      {"path": "templates/typescript/", "purpose": "Code templates"},
      {"path": "workflows/", "purpose": "Workflow documentation"},
      {"path": "src/components/", "purpose": "React components"},
      {"path": "src/hooks/", "purpose": "Custom hooks"},
      {"path": "src/services/", "purpose": "API services"},
      {"path": "src/types/", "purpose": "TypeScript types"},
      {"path": "tests/", "purpose": "Test files"}
    ],
    "files": [
      {"path": ".cursorrules", "purpose": "Agent behavior rules"},
      {"path": "README.md", "purpose": "Project documentation"},
      {"path": "package.json", "purpose": "NPM config"},
      {"path": "tsconfig.json", "purpose": "TypeScript config"}
    ]
  },
  "cursorrules": {
    "variables": [
      {"name": "NODE_VERSION", "description": "Node.js version", "default": "20"},
      {"name": "STYLE_GUIDE", "description": "Coding style", "default": "airbnb"}
    ],
    "rules": [
      {"name": "TypeScript Strict", "description": "Use strict TypeScript mode with no any types"},
      {"name": "React 19", "description": "Use React 19 features including Actions, useOptimistic, and useFormStatus"},
      {"name": "Component Patterns", "description": "Use functional components with hooks, prefer composition over inheritance"},
      {"name": "Type Safety", "description": "Define proper TypeScript types for props, state, and API responses"},
      {"name": "Testing", "description": "Write Vitest unit tests with React Testing Library"}
    ]
  }
}
