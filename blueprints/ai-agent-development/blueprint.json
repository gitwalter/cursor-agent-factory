{
  "metadata": {
    "blueprintId": "ai-agent-development",
    "blueprintName": "AI Agent Development Blueprint",
    "description": "Build AI agent systems with LangChain, LangGraph, and modern LLM frameworks",
    "version": "2.0.0",
    "author": "Cursor Agent Factory",
    "tags": ["python", "ai", "agents", "langchain", "langgraph", "llm", "ml"],
    "purpose": "Enable developers to create intelligent, coordinated AI agent systems that serve users with integrity"
  },
  "stack": {
    "primaryLanguage": "python",
    "frameworks": [
      {"name": "LangChain", "version": "0.3+", "purpose": "Agent framework and chains"},
      {"name": "LangGraph", "version": "0.2+", "purpose": "Stateful agent orchestration"},
      {"name": "CrewAI", "version": "0.50+", "purpose": "Multi-agent crew orchestration"},
      {"name": "AutoGen", "version": "0.2+", "purpose": "Conversational multi-agent systems"},
      {"name": "Streamlit", "version": "1.30+", "purpose": "Interactive UI"},
      {"name": "FastAPI", "version": "0.100+", "purpose": "API endpoints"},
      {"name": "Pydantic", "version": "2.0+", "purpose": "Data validation and structured outputs"}
    ],
    "databases": [
      {"type": "vector", "name": "ChromaDB", "purpose": "Vector store for RAG"},
      {"type": "vector", "name": "FAISS", "purpose": "Alternative vector store"},
      {"type": "sql", "name": "SQLite", "purpose": "Prompt/state storage"},
      {"type": "sql", "name": "PostgreSQL", "purpose": "Production database"}
    ],
    "llmProviders": [
      {"name": "OpenAI", "models": ["gpt-4o", "gpt-4o-mini", "o1", "o1-mini"], "purpose": "Primary LLM"},
      {"name": "Anthropic", "models": ["claude-3-5-sonnet", "claude-3-opus", "claude-3-haiku"], "purpose": "Alternative LLM"},
      {"name": "Google", "models": ["gemini-2.0-flash", "gemini-1.5-pro"], "purpose": "Long context LLM"},
      {"name": "Ollama", "models": ["llama3.2", "mistral", "codellama"], "purpose": "Local LLM"}
    ],
    "tools": [
      {"name": "pytest", "purpose": "Testing"},
      {"name": "ruff", "purpose": "Linting"},
      {"name": "mypy", "purpose": "Type checking"},
      {"name": "langsmith", "purpose": "Agent tracing and debugging"},
      {"name": "poetry", "purpose": "Dependency management"}
    ],
    "styleGuides": [
      {"name": "pep8", "description": "PEP 8 Style Guide", "reference": "https://pep8.org"},
      {"name": "google", "description": "Google Python Style", "reference": "https://google.github.io/styleguide/pyguide.html"}
    ]
  },
  "agents": [
    {"patternId": "code-reviewer", "required": true, "customizations": {"skills": ["clean-code-review", "grounding"]}},
    {"patternId": "test-generator", "required": true, "customizations": {"skills": ["tdd"]}},
    {"patternId": "documentation-agent", "required": true, "customizations": {"focus": "agent-documentation"}},
    {"patternId": "knowledge-extender", "required": true, "description": "Extend knowledge base during development"},
    {"patternId": "knowledge-evolution", "required": true, "description": "Manage and evolve project knowledge independently"},
    {"patternId": "factory-updates", "required": true, "description": "Receive updates from the Cursor Agent Factory"},
    {"patternId": "explorer", "required": false, "customizations": {"focus": "research-papers"}}
  ],
  "skills": [
    {"patternId": "prompt-engineering", "required": true},
    {"patternId": "agent-coordination", "required": true},
    {"patternId": "grounding", "required": true},
    {"patternId": "tdd", "required": true},
    {"patternId": "bugfix-workflow", "required": true},
    {"patternId": "feature-workflow", "required": true},
    {"patternId": "extend-knowledge", "required": true, "description": "Extend knowledge base with new topics"},
    {"patternId": "receive-updates", "required": true, "description": "Receive updates from Factory"}
  ],
  "knowledge": [
    {"filename": "langchain-patterns.json", "description": "LangChain best practices and patterns"},
    {"filename": "langgraph-workflows.json", "description": "LangGraph state machine patterns"},
    {"filename": "crewai-patterns.json", "description": "CrewAI crew orchestration and agent patterns"},
    {"filename": "autogen-patterns.json", "description": "AutoGen conversational agent patterns"},
    {"filename": "mcp-patterns.json", "description": "Model Context Protocol (MCP) server integration patterns"},
    {"filename": "agent-testing.json", "description": "Agent testing, evaluation, and benchmarking patterns"},
    {"filename": "prompt-engineering.json", "description": "Prompt optimization techniques"},
    {"filename": "agent-coordination.json", "description": "Multi-agent coordination patterns"},
    {"filename": "llm-provider-comparison.json", "description": "LLM provider comparison and selection"},
    {"filename": "multi-agent-patterns.json", "description": "Consolidated multi-agent coordination patterns"},
    {"filename": "rag-patterns.json", "description": "RAG architecture and retrieval patterns"},
    {"filename": "vector-database-patterns.json", "description": "Vector store selection and usage"},
    {"filename": "llm-evaluation-patterns.json", "description": "LLM and RAG evaluation patterns"},
    {"filename": "prompt-library.json", "description": "Reusable prompt templates"},
    {"filename": "huggingface-patterns.json", "description": "Hugging Face Transformers patterns"}
  ],
  "templates": {
    "codeTemplates": [
      {"category": "agent-base", "directory": "templates/ai/agent/"},
      {"category": "tool-definition", "directory": "templates/ai/tool/"},
      {"category": "prompt-template", "directory": "templates/ai/prompt/"},
      {"category": "workflow-graph", "directory": "templates/ai/workflow/"},
      {"category": "test-class", "directory": "templates/python/test/"}
    ],
    "documentTemplates": [
      {"name": "agent_design.md", "directory": "templates/docs/"},
      {"name": "prompt_library.md", "directory": "templates/docs/"},
      {"name": "implementation_plan.md", "directory": "templates/docs/"}
    ]
  },
  "workflows": [
    {"patternId": "agent-development", "customizations": {}},
    {"patternId": "prompt-iteration", "customizations": {}},
    {"patternId": "jira-bugfix", "customizations": {}}
  ],
  "mcpServers": [
    {"name": "deepwiki", "url": "https://mcp.deepwiki.com/mcp", "purpose": "GitHub analysis", "required": true},
    {"name": "atlassian", "url": "https://mcp.atlassian.com/v1/sse", "purpose": "Jira/Confluence", "required": false}
  ],
  "mcpIntegration": {
    "patterns": [
      {"name": "mcp-server-setup", "description": "Setting up MCP servers for agent tools"},
      {"name": "mcp-resource-handling", "description": "Handling MCP resources in agent workflows"},
      {"name": "mcp-tool-integration", "description": "Integrating MCP tools into agent toolkits"},
      {"name": "mcp-gateway-pattern", "description": "Gateway pattern for MCP server communication"}
    ],
    "knowledgeFile": "knowledge/mcp-patterns.json",
    "templates": [
      {"category": "mcp-server", "directory": "templates/ai/mcp/"}
    ]
  },
  "projectStructure": {
    "directories": [
      {"path": ".cursor/agents/", "purpose": "AI agent definitions for Cursor"},
      {"path": ".cursor/skills/", "purpose": "Reusable skills for Cursor"},
      {"path": "agents/", "purpose": "Agent implementations"},
      {"path": "agents/core/", "purpose": "Base agent classes"},
      {"path": "agents/specialized/", "purpose": "Domain-specific agents"},
      {"path": "prompts/", "purpose": "Prompt templates"},
      {"path": "prompts/templates/", "purpose": "Prompt template files"},
      {"path": "prompts/system/", "purpose": "System prompts"},
      {"path": "workflow/", "purpose": "LangGraph workflows"},
      {"path": "tools/", "purpose": "Agent tools"},
      {"path": "knowledge/", "purpose": "Knowledge base and RAG data"},
      {"path": "apps/", "purpose": "UI applications (Streamlit)"},
      {"path": "api/", "purpose": "FastAPI endpoints"},
      {"path": "tests/", "purpose": "Test files"},
      {"path": "tests/agents/", "purpose": "Agent tests"},
      {"path": "tests/prompts/", "purpose": "Prompt tests"},
      {"path": "docs/", "purpose": "Documentation"},
      {"path": "workflows/", "purpose": "Methodology documentation"}
    ],
    "files": [
      {"path": ".cursorrules", "purpose": "Agent behavior rules (5-layer)"},
      {"path": "PURPOSE.md", "purpose": "Purpose document (Layer 1)"},
      {"path": "README.md", "purpose": "Project documentation"},
      {"path": "pyproject.toml", "purpose": "Python project config"},
      {"path": "workflows/methodology.yaml", "purpose": "Methodology configuration"}
    ]
  },
  "cursorrules": {
    "variables": [
      {"name": "PYTHON_PATH", "description": "Python executable path", "default": "C:\\App\\Anaconda\\python.exe"},
      {"name": "STYLE_GUIDE", "description": "Coding style", "default": "google"},
      {"name": "LLM_PROVIDER", "description": "Default LLM provider", "default": "openai"},
      {"name": "LANGSMITH_TRACING", "description": "Enable LangSmith tracing", "default": "true"}
    ],
    "rules": [
      {"name": "Type Hints", "description": "Always use type hints for function parameters and returns"},
      {"name": "Docstrings", "description": "Use Google-style docstrings for all public functions"},
      {"name": "Structured Outputs", "description": "Use Pydantic models for LLM structured outputs"},
      {"name": "Error Handling", "description": "Implement proper error handling with fallbacks"},
      {"name": "Tracing", "description": "Add LangSmith tracing to all agent operations"}
    ]
  },
  "layerDefaults": {
    "layer0": {
      "coreAxioms": ["A1", "A2", "A3", "A4", "A5"],
      "optionalAxioms": ["A10"],
      "note": "A10 (Learning) is essential for AI agent development - continuous improvement from feedback"
    },
    "layer1": {
      "purposeTemplate": "To create intelligent AI agents that serve users with integrity, transparency, and continuous improvement",
      "stakeholders": "Developers building AI-powered applications"
    },
    "layer3": {
      "methodology": "research-development",
      "note": "R&D methodology suits experimental AI development with 70/30 exploration/exploitation"
    }
  },
  "axiomAlignment": {
    "A1_verifiability": "All agent outputs include reasoning traces via LangSmith",
    "A2_user_primacy": "Agents always confirm before taking consequential actions",
    "A3_transparency": "Agent decisions are explainable through prompt inspection",
    "A4_non_harm": "Agents refuse harmful requests and validate safety constraints",
    "A5_consistency": "Agent behavior derives from documented prompts and rules",
    "A10_learning": "Feedback loops improve agent performance over time"
  }
}
