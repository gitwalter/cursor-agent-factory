{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Project Information",
  "description": "Metadata about this project's Factory origin and installed components",
  "version": "1.0.0",
  "project": {
    "name": "{PROJECT_NAME}",
    "created": "{CREATED_DATE}",
    "description": "{PROJECT_DESCRIPTION}"
  },
  "factory_origin": {
    "factory_version": "{FACTORY_VERSION}",
    "blueprint_id": "{BLUEPRINT_ID}",
    "generated_date": "{GENERATED_DATE}",
    "feed_url": "https://raw.githubusercontent.com/gitwalter/cursor-agent-factory/main/knowledge/factory-updates.json"
  },
  "update_preferences": {
    "channel": "stable",
    "auto_check": false,
    "notify_only": true,
    "subscriptions": ["all"]
  },
  "installed_updates": [],
  "installed_knowledge": {},
  "last_update_check": null
}
