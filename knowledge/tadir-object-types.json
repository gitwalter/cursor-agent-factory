{
  "metadata": {
    "name": "TADIR Object Types",
    "description": "SAP Repository object types from TADIR for ABAP development",
    "version": "1.0.0",
    "lastUpdated": "2026-01-30"
  },
  "commonObjectTypes": {
    "CLAS": {
      "description": "ABAP Class",
      "package": "Required",
      "naming": "ZCL_* or YCL_*",
      "components": ["Public/Protected/Private sections", "Methods", "Attributes", "Events", "Types"]
    },
    "INTF": {
      "description": "ABAP Interface",
      "package": "Required",
      "naming": "ZIF_* or YIF_*",
      "components": ["Methods", "Attributes", "Events", "Types"]
    },
    "PROG": {
      "description": "ABAP Program/Report",
      "package": "Required",
      "naming": "Z* or Y*",
      "types": ["Executable (type 1)", "Include (type I)", "Module pool (type M)"]
    },
    "FUGR": {
      "description": "Function Group",
      "package": "Required",
      "naming": "Z* or Y*",
      "contains": ["Function modules", "Global data", "Includes"]
    },
    "FUNC": {
      "description": "Function Module",
      "container": "Function Group (FUGR)",
      "naming": "Z_* or Y_*",
      "types": ["Normal", "RFC-enabled", "Update module"]
    },
    "TABL": {
      "description": "Database Table",
      "package": "Required",
      "naming": "Z* or Y*",
      "types": ["Transparent", "Cluster", "Pool"]
    },
    "VIEW": {
      "description": "Database View",
      "package": "Required",
      "naming": "Z* or Y*",
      "types": ["Database view", "Projection view", "Help view", "Maintenance view"]
    },
    "DDLS": {
      "description": "CDS Data Definition",
      "package": "Required",
      "naming": "Z* or Y*",
      "types": ["CDS View", "CDS Entity", "CDS Table Function", "CDS Abstract Entity"]
    },
    "DCLS": {
      "description": "CDS Access Control",
      "package": "Required",
      "purpose": "Row-level security for CDS views"
    },
    "DDLX": {
      "description": "CDS Metadata Extension",
      "package": "Required",
      "purpose": "Separate annotations from CDS definition"
    },
    "BDEF": {
      "description": "Behavior Definition",
      "package": "Required",
      "naming": "Z*_B or ZBP_*",
      "purpose": "RAP behavior definition"
    },
    "SRVB": {
      "description": "Service Binding",
      "package": "Required",
      "purpose": "OData service exposure"
    },
    "SRVD": {
      "description": "Service Definition",
      "package": "Required",
      "purpose": "Service exposure definition"
    },
    "DOMA": {
      "description": "Domain",
      "package": "Required",
      "naming": "Z* or Y*",
      "purpose": "Value range and format definition"
    },
    "DTEL": {
      "description": "Data Element",
      "package": "Required",
      "naming": "Z* or Y*",
      "purpose": "Semantic data type with labels"
    },
    "TTYP": {
      "description": "Table Type",
      "package": "Required",
      "naming": "Z* or Y*",
      "purpose": "Internal table type definition"
    },
    "MSAG": {
      "description": "Message Class",
      "package": "Required",
      "naming": "Z* or Y*",
      "contains": ["Message numbers 000-999", "Language-dependent texts"]
    },
    "ENHO": {
      "description": "Enhancement Implementation",
      "package": "Required",
      "purpose": "Customer enhancements to SAP code"
    },
    "ENHS": {
      "description": "Enhancement Spot",
      "package": "Required",
      "purpose": "Enhancement point definition"
    },
    "SHLP": {
      "description": "Search Help",
      "package": "Required",
      "naming": "Z* or Y*",
      "purpose": "Value help for input fields"
    },
    "TRAN": {
      "description": "Transaction Code",
      "package": "Required",
      "naming": "Z* or Y*",
      "types": ["Dialog", "Report", "OO Transaction", "Variant transaction"]
    },
    "XSLT": {
      "description": "XSLT Program",
      "package": "Required",
      "purpose": "XML transformations"
    },
    "NROB": {
      "description": "Number Range Object",
      "package": "Required",
      "purpose": "Number range management"
    }
  },
  "rapObjectTypes": {
    "DDLS": "CDS View/Entity for data model",
    "BDEF": "Behavior Definition for RAP BO",
    "CLAS": "Behavior Implementation class",
    "SRVD": "Service Definition for exposure",
    "SRVB": "Service Binding (OData V2/V4)",
    "DCLS": "Access Control for authorization"
  },
  "packageTypes": {
    "DEVC": {
      "description": "Development Package",
      "naming": "Z* or Y* or $TMP (local)",
      "purpose": "Group and transport related objects"
    }
  },
  "transportRelevance": {
    "transportable": ["CLAS", "INTF", "PROG", "FUGR", "TABL", "DDLS", "BDEF", "SRVB", "SRVD"],
    "localOnly": ["Objects in $TMP package"],
    "systemGenerated": ["Generated proxies", "Runtime objects"]
  },
  "objectRelations": {
    "cdsToTable": "DDLS references TABL",
    "behaviorToCds": "BDEF references DDLS",
    "serviceToBehavior": "SRVD references BDEF",
    "bindingToService": "SRVB references SRVD",
    "classToInterface": "CLAS implements INTF"
  },
  "whereUsedRelevance": {
    "highlyRelevant": ["CLAS", "INTF", "DDLS", "TABL", "DTEL"],
    "mediumRelevant": ["FUNC", "PROG", "BDEF"],
    "lessRelevant": ["MSAG", "SHLP", "TRAN"]
  }
}
