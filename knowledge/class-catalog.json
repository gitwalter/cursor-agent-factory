{
  "metadata": {
    "catalogId": "class-catalog",
    "catalogName": "ABAP Classes Catalog",
    "description": "Pre-verified ABAP classes, interfaces, and exception classes with release contracts",
    "version": "1.0.0",
    "lastUpdated": "2025-01-28",
    "sources": ["SAP Help Portal", "ABAP Documentation"]
  },
  "classes": {
    "CL_ABAP_TYPEDESCR": {
      "type": "class",
      "abstract": true,
      "final": false,
      "description": "Abstract base class for ABAP type descriptors",
      "released": {"C1": true},
      "methods": {
        "DESCRIBE_BY_DATA": {
          "static": true,
          "visibility": "public",
          "description": "Get type descriptor for data object",
          "importing": [
            {"name": "P_DATA", "type": "ANY", "optional": false}
          ],
          "returning": {"name": "P_DESCR_REF", "type": "REF TO CL_ABAP_TYPEDESCR"}
        },
        "DESCRIBE_BY_NAME": {
          "static": true,
          "visibility": "public",
          "description": "Get type descriptor by type name",
          "importing": [
            {"name": "P_NAME", "type": "STRING", "optional": false}
          ],
          "returning": {"name": "P_DESCR_REF", "type": "REF TO CL_ABAP_TYPEDESCR"},
          "raising": ["CX_SY_RTTI_SYNTAX_ERROR"]
        },
        "GET_RELATIVE_NAME": {
          "static": false,
          "visibility": "public",
          "description": "Get relative type name",
          "returning": {"name": "P_RELATIVE_NAME", "type": "ABAP_ABSTYPENAME"}
        }
      },
      "source": "SAP Help Portal"
    },
    "CL_ABAP_STRUCTDESCR": {
      "type": "class",
      "abstract": false,
      "final": true,
      "superclass": "CL_ABAP_COMPLEXDESCR",
      "description": "Type descriptor for structures",
      "released": {"C1": true},
      "methods": {
        "GET_COMPONENTS": {
          "static": false,
          "visibility": "public",
          "description": "Get structure components",
          "returning": {"name": "P_RESULT", "type": "ABAP_COMPONENT_TAB"}
        },
        "GET_INCLUDED_VIEW": {
          "static": false,
          "visibility": "public",
          "description": "Get flattened view of structure",
          "returning": {"name": "P_RESULT", "type": "ABAP_COMPONENT_VIEW_TAB"}
        }
      },
      "source": "SAP Help Portal"
    },
    "CL_ABAP_TABLEDESCR": {
      "type": "class",
      "abstract": false,
      "final": true,
      "superclass": "CL_ABAP_COMPLEXDESCR",
      "description": "Type descriptor for internal tables",
      "released": {"C1": true},
      "methods": {
        "GET_TABLE_LINE_TYPE": {
          "static": false,
          "visibility": "public",
          "description": "Get line type descriptor",
          "returning": {"name": "P_LINE_TYPE", "type": "REF TO CL_ABAP_DATADESCR"}
        },
        "GET_KEYS": {
          "static": false,
          "visibility": "public",
          "description": "Get table keys",
          "returning": {"name": "P_KEYS", "type": "ABAP_KEYDESCR_TAB"}
        }
      },
      "source": "SAP Help Portal"
    },
    "CL_SALV_TABLE": {
      "type": "class",
      "abstract": false,
      "final": false,
      "description": "ALV Table Display",
      "released": {"C1": true},
      "methods": {
        "FACTORY": {
          "static": true,
          "visibility": "public",
          "description": "Create ALV table instance",
          "importing": [
            {"name": "LIST_DISPLAY", "type": "ABAP_BOOL", "optional": true},
            {"name": "R_CONTAINER", "type": "REF TO CL_GUI_CONTAINER", "optional": true}
          ],
          "exporting": [
            {"name": "R_SALV_TABLE", "type": "REF TO CL_SALV_TABLE"}
          ],
          "changing": [
            {"name": "T_TABLE", "type": "ANY TABLE"}
          ],
          "raising": ["CX_SALV_MSG"]
        },
        "DISPLAY": {
          "static": false,
          "visibility": "public",
          "description": "Display ALV table"
        },
        "GET_COLUMNS": {
          "static": false,
          "visibility": "public",
          "description": "Get columns object",
          "returning": {"name": "VALUE", "type": "REF TO CL_SALV_COLUMNS_TABLE"}
        }
      },
      "source": "SAP Help Portal"
    }
  },
  "interfaces": {
    "IF_ABAP_BEHV": {
      "description": "RAP Behavior Handler Interface",
      "released": {"C1": true},
      "methods": {},
      "source": "SAP Help Portal"
    },
    "IF_ABAP_BEHV_MESSAGE": {
      "description": "RAP Behavior Message Interface",
      "released": {"C1": true},
      "methods": {
        "GET_TEXT": {
          "description": "Get message text",
          "returning": {"name": "RESULT", "type": "STRING"}
        }
      },
      "source": "SAP Help Portal"
    },
    "IF_T100_MESSAGE": {
      "description": "Interface for T100 Messages",
      "released": {"C1": true},
      "attributes": {
        "T100KEY": {
          "type": "SCX_T100KEY",
          "description": "Message key structure"
        }
      },
      "source": "SAP Help Portal"
    },
    "IF_T100_DYN_MSG": {
      "description": "Interface for Dynamic T100 Messages",
      "released": {"C1": true},
      "includes": ["IF_T100_MESSAGE"],
      "attributes": {
        "MSGTY": {"type": "SYMSGTY", "description": "Message Type"},
        "MSGV1": {"type": "SYMSGV", "description": "Message Variable 1"},
        "MSGV2": {"type": "SYMSGV", "description": "Message Variable 2"},
        "MSGV3": {"type": "SYMSGV", "description": "Message Variable 3"},
        "MSGV4": {"type": "SYMSGV", "description": "Message Variable 4"}
      },
      "source": "SAP Help Portal"
    }
  },
  "exceptionClasses": {
    "CX_ROOT": {
      "description": "Root exception class",
      "abstract": true,
      "released": {"C1": true},
      "attributes": {
        "PREVIOUS": {"type": "REF TO CX_ROOT", "description": "Previous exception in chain"}
      },
      "methods": {
        "GET_TEXT": {
          "returning": {"name": "RESULT", "type": "STRING"},
          "description": "Get exception text"
        }
      },
      "source": "SAP Help Portal"
    },
    "CX_STATIC_CHECK": {
      "description": "Base for checked exceptions",
      "abstract": true,
      "superclass": "CX_ROOT",
      "released": {"C1": true},
      "source": "SAP Help Portal"
    },
    "CX_DYNAMIC_CHECK": {
      "description": "Base for dynamic exceptions",
      "abstract": true,
      "superclass": "CX_ROOT",
      "released": {"C1": true},
      "source": "SAP Help Portal"
    },
    "CX_NO_CHECK": {
      "description": "Base for runtime exceptions",
      "abstract": true,
      "superclass": "CX_ROOT",
      "released": {"C1": true},
      "source": "SAP Help Portal"
    },
    "CX_SY_ITAB_LINE_NOT_FOUND": {
      "description": "Table line not found exception",
      "superclass": "CX_SY_ITAB_ERROR",
      "released": {"C1": true},
      "source": "SAP Help Portal"
    }
  },
  "usage": {
    "description": "Use this catalog to verify ABAP classes and interfaces before use",
